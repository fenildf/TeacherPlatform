<body class="skin skin-green">
	<h1 class="ui_infor_title">
		<span>项目列表</span>
	</h1>
	<div class="action_item">

		<ul class="action">
			<li>
			<form action="" name="" id="listSerch">
				<select name="" id="selectGrade" class="ui-select-box">
					<option>--年级--</option>
					<option value="1">一年级</option>
					<option value="2">二年级</option>
					<option value="3">三年级</option>
					<option value="4">四年级</option>
					<option value="5">五年级</option>
					<option value="5">六年级</option>
				</select>
				<select name="" id="selectCourseType" class="ui-select-box">
					<option>--项目状态--</option>
					<option value="1">已上架</option>
					<option value="2">未上架</option>
					<option value="3">已审核</option>
					<option value="4">未审核</option>
				</select>
				<input type="submit" value="筛选" class="s-btn">
				<input type="hidden" class="search_key" title="存储头部搜索结果" />
				<input type="hidden" id="currpage" class="currpage" name="currpage" value="1" />
			</form>
			</li>
		</ul>
		<ul class="item_pag ui_pages">
			<li class="page_list ui-select-box">
				<select name="pages" id="pages" class="ui-select">
					<option value="1">1/5</option>
					<option value="2">2/5</option>
					<option value="3">3/5</option>
					<option value="4">4/5</option>
					<option value="5">5/5</option>
				</select>
			</li>
			<li class="page_btn">
				<span href="" class="page_prev">上一页</span>
			</li>
			<li class="page_btn">
				<span class="page_next">下一页</span>
			</li>
		</ul>
		
	</div>
	<ul class="list_button">
		<li><a id="project_create" href="project_create.html" class="open_tabs">创建项目</a></li>
		<li><a id="project_record_all" href="project_record.html" class="open_tabs">提交审核</a></li>
	</ul>
